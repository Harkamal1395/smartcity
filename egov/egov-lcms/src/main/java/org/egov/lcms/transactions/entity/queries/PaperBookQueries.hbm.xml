<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

 <query name="getPaperBookByLegalCaseId">
    <![CDATA[from PaperBook pb where pb.legalcase.id=?]]></query>

 <query name="getPaperBookById">
    <![CDATA[from PaperBook pb where pb.id=?]]></query>
    

</hibernate-mapping>